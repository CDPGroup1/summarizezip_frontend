{
  "name": "SummarizeZip",
  "description": "Build an Extension!",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_icon": {
      "16": "/images/icon-16x16.png",
      "32": "/images/icon-32x32.png",
      "48": "/images/icon-48x48.png",
      "128": "/images/icon-128x128.png"
    }
  },
  "icons": {
    "16": "/images/icon-16x16.png",
    "32": "/images/icon-32x32.png",
    "48": "/images/icon-48x48.png",
    "128": "/images/icon-128x128.png"
  }
}
